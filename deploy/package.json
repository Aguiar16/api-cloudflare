{
  "name": "@smol-frontend/deploy-cloudflare",
  "version": "0.0.2",
  "author": "Adrien Baron <adrien@abaron.net>",
  "description": "Publish a smol frontend to a smol frontend API hosted on Cloudflare",
  "keywords": ["smol","micro frontend","mfe"],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "homepage": "https://github.com/smol-frontend/api-cloudflare#readme",
  "repository": "git@github.com:smol-frontend/api-cloudflare.git",
  "license": "MIT",
  "devDependencies": {
    "@cazoo/eslint-plugin-eslint": "^1.0.2",
    "@types/node": "^17.0.10",
    "eslint": "^7.32.0",
    "typescript": "^4.5.5",
    "vite": "^2.7.2"
  },
  "type": "module",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/deploy-cloudflare.umd.js",
  "module": "./dist/deploy-cloudflare.es.js",
  "exports": {
    ".": {
      "import": "./dist/deploy-cloudflare.es.js",
      "require": "./dist/deploy-cloudflare.umd.js"
    }
  },
  "dependencies": {
    "node-fetch": "^3.2.0"
  }
}
